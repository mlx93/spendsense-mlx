{
  "name": "spendsense",
  "version": "1.0.0",
  "description": "Explainable, consent-aware financial education platform",
  "private": true,
  "scripts": {
    "dev": "npm run setup && npm run dev:all",
    "setup": "npm install && npm run db:migrate && npm run db:generate && npm run db:seed",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:generate": "cd backend && npx prisma generate",
    "db:seed": "cd backend && npx prisma db seed",
    "db:reset": "cd backend && npx prisma migrate reset --force",
    "seed": "npm run db:seed",
    "export:csv": "cd backend && npm run export:csv",
    "export:json": "cd backend && npm run export:json",
    "export:parquet": "cd backend && npm run export:parquet",
    "test": "cd backend && npm test",
    "test:watch": "cd backend && npm run test:watch",
    "test:coverage": "cd backend && npm run test:coverage",
    "eval": "cd backend && npm run eval",
    "eval:metrics": "cd backend && npm run eval:metrics",
    "eval:report": "cd backend && npm run eval:report",
    "eval:traces": "cd backend && npm run eval:traces"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlx93/spendsense-mlx.git"
  },
  "keywords": [
    "financial-education",
    "plaid",
    "personas",
    "recommendations",
    "explainable-ai"
  ],
  "author": "",
  "license": "MIT"
}

