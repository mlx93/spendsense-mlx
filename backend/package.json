{
  "name": "spendsense-backend",
  "version": "1.0.0",
  "description": "SpendSense backend API server",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eval": "tsx scripts/eval-harness.ts",
    "eval:metrics": "tsx scripts/eval-harness.ts --metrics",
    "eval:report": "tsx scripts/eval-harness.ts --report",
    "eval:traces": "tsx scripts/eval-harness.ts --traces",
    "export:csv": "tsx scripts/export-csv.ts",
    "export:json": "tsx scripts/export-json.ts",
    "export:parquet": "tsx scripts/export-parquet.ts",
    "reseed:transactions": "tsx scripts/reseed-transactions.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.20.0",
    "seedrandom": "^3.0.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/seedrandom": "^3.0.8",
    "jest": "^29.7.0",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
